{
  "name": "twilio-paste-intellisense",
  "displayName": "Twilio Paste Intellisense",
  "description": "Intelligent Twilio Paste Design tooling for VS Code",
  "version": "0.1.2",
  "homepage": "https://github.com/Gapur/twilio-paste-intellisense",
  "bugs": {
    "url": "https://github.com/Gapur/twilio-paste-intellisense/issues",
    "email": "gapur.kassym@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gapur/twilio-paste-intellisense.git"
  },
  "publisher": "Gapur",
  "keywords": [
    "twilio",
    "twilio-paste",
    "paste",
    "css",
    "intellisense",
    "autocomplete",
    "vscode"
  ],
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Linters",
    "Other"
  ],
  "icon": "assets/favicon.png",
  "galleryBanner": {
    "color": "#f9fafb"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "typescript": "^4.6.4",
    "@vscode/test-electron": "^2.1.3"
  }
}
